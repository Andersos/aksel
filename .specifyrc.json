{
  "repository": "@nav/designsystem",
  "head": "updated-specify-tokens",
  "base": "master",
  "rules": [
    {
      "name": "Specify / Icons",
      "path": "@navikt/ds-icons/svg",
      "filter": {
        "types": ["vector"]
      },
      "parsers": [
        {
          "name": "name-assets-files-by-pattern",
          "options": {
            "pattern": "{{name}}.{{format}}"
          }
        }
      ]
    }
  ]
}
